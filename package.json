{
  "name": "rate-repository-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@expo/metro-config": "^0.3.22",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-picker/picker": "2.2.1",
    "dotenv": "^16.0.2",
    "expo": "~44.0.0",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-linking": "~3.0.0",
    "expo-status-bar": "~1.2.0",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "jest": "^29.3.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-unimodules": "~0.15.0",
    "react-native-web": "0.17.1",
    "react-router-dom": "^6.3.0",
    "react-router-native": "^6.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.18.9",
    "@testing-library/jest-native": "^5.1.1",
    "@testing-library/react-native": "^11.3.0",
    "@types/jest": "^29.2.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "eslint": "^8.23.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-native": "^4.0.0",
    "jest-expo": "^46.0.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "~4.3.5"
  },
  "private": true
}
